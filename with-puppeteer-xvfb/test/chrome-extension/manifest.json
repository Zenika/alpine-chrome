{
    "name": "Chrome Sample Extension",
    "version": "1.0.0",
    "description": "Sample Extension to turn the background in red",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_start",
            "js": [
                "content-script.js"
            ]
        }
    ]
}